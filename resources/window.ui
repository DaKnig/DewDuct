<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.12.1 -->
<interface>
  <!-- interface-name window.ui -->
  <requires lib="gtk" version="4.8"/>
  <requires lib="libadwaita" version="1.3"/>
  <template class="DewDuctWindow" parent="AdwApplicationWindow">
    <property name="default-height">360</property>
    <property name="default-width">294</property>
    <property name="title">DewDuct</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar">
            <child type="start">
              <object class="GtkToggleButton">
                <property name="active" bind-source="flap" bind-property="reveal-flap" bind-flags="bidirectional | sync-create"/>
                <property name="icon-name">open-menu-symbolic</property>
              </object>
            </child>
            <child type="start">
              <object class="GtkButton" id="back_button">
                <property name="action-name">win.back</property>
                <property name="icon-name">go-previous-symbolic</property>
                <property name="visible" bind-source="video_page" bind-property="visible" bind-flags="bidirectional | sync-create">True</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwFlap" id="flap">
            <property name="flap">
              <object class="GtkStackSidebar">
                <property name="stack">screen_stack</property>
                <property name="vexpand">True</property>
                <style>
                  <class name="background"/>
                </style>
              </object>
            </property>
            <property name="fold-threshold-policy">natural</property>
            <property name="reveal-flap">False</property>
            <child type="content">
              <object class="GtkStack" id="screen_stack">
                <child>
                  <object class="GtkStackPage" id="normal_view_page">
                    <property name="child">
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="AdwViewSwitcherBar">
                            <property name="reveal">true</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwViewStack" id="view-stack">
                            <child>
                              <object class="DewUpdatePage" id="update_page"/>
                            </child>
                            <child>
                              <object class="GtkBox" id="subscribe-page"/>
                            </child>
                          </object>
                        </child>
                      </object>
                    </property>
                    <property name="name">normal_view_page</property>
                    <property name="title">Home</property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="child">
                      <object class="DewVideoPage" id="video_page">
                        <property name="visible">False</property>
                      </object>
                    </property>
                    <property name="icon-name">audio-x-generic</property>
                    <property name="name">video_page</property>
                    <property name="title">Player</property>
                    <property name="visible" bind-source="video_page" bind-property="visible" bind-flags="bidirectional | sync-create">False</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
