<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.12.1 -->
<interface>
  <!-- interface-name window.ui -->
  <requires lib="gtk" version="4.8"/>
  <requires lib="libadwaita" version="1.3"/>
  <template class="DewDuctWindow" parent="AdwApplicationWindow">
    <property name="default-height">360</property>
    <property name="default-width">294</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar">
            <child>
              <object class="GtkToggleButton">
                <property name="active" bind-source="flap" bind-property="reveal-flap" bind-flags="sync-create|bidirectional"/>
                <property name="icon-name">open-menu-symbolic</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwFlap" id="flap">
            <child type="flap">
              <object class="GtkStackSidebar" id="flap-flap">
                <property name="stack">screen-stack</property>
                <property name="vexpand">True</property>
              </object>
            </child>
            <child type="content">
              <object class="GtkStack" id="screen-stack">
                <property name="transition-type">0</property>
                <child>
                  <object class="GtkBox" id="normal-view">
                    <property name="orientation">1</property>
                    <child>
                      <object class="AdwViewSwitcherBar">
                        <property name="reveal">true</property>
                      </object>
                    </child>
                    <child>
                      <object class="AdwViewStack" id="view-stack">
                        <child>
                          <object class="DewUpdatePage"/>
                        </child>
                        <child>
                          <object class="GtkBox" id="subscribe-page"/>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
