<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.12.1 -->
<interface>
  <!-- interface-name update_page.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.3"/>
  <template class="DewUpdatePage" parent="GtkBox">
    <property name="orientation">1</property>
    <child>
      <object class="GtkButton" id="update_button">
        <property name="label">Last updated: never</property>
        <signal name="clicked" handler="update_vids" swapped="yes"/>
      </object>
    </child>
    <child>
      <object class="GtkScrolledWindow">
        <property name="vexpand">True</property>
        <child>
          <object class="AdwClampScrollable">
            <child>
              <object class="GtkListView" id="new_vids">
                <property name="factory">
                  <object class="GtkSignalListItemFactory" id="vid_factory">
                    <signal name="setup" handler="setup_vid_widget" swapped="yes"/>
                    <signal name="bind" handler="bind_vid_widget" swapped="yes"/>
                  </object>
                </property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
