<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.12.1 -->
<interface>
  <requires lib="gtk" version="4.10"/>
  <template class="DewVideoRow" parent="GtkBox">
    <property name="height-request">80</property>
    <property name="hexpand">True</property>
    <child>
      <object class="DewThumbnail" id="thumbnail"/>
    </child>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="title">
            <property name="ellipsize">end</property>
            <property name="halign">start</property>
            <property name="label">the amazing video that will change your life!</property>
            <property name="lines">2</property>
            <property name="valign">start</property>
            <property name="xalign">0.0</property>
            <property name="yalign">0.0</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel">
            <property name="label">   </property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="channel">
            <property name="ellipsize">end</property>
            <property name="halign">start</property>
            <property name="label">this subtitle should be the channel</property>
            <property name="sensitive">False</property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <child>
              <object class="GtkLabel" id="views">
                <property name="ellipsize">start</property>
                <property name="label">100k</property>
                <property name="sensitive">False</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="published">
                <property name="ellipsize">end</property>
                <property name="halign">end</property>
                <property name="label">1 hour ago</property>
                <property name="margin-start">10</property>
                <property name="sensitive">False</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
